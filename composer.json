{
  "require": {
    "vlucas/phpdotenv": "^2.4",
    "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Src\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "src/Tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "extra": {
    "hooks": {
      "pre-commit": [
        "./vendor/bin/phpunit src"
      ]
    }
  }
}
